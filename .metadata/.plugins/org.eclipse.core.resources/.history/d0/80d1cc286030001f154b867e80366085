package StringProgram;

import java.util.LinkedHashSet;

public class Demo {

	public static void main(String[] args) {

        String s="java";  

// LinkedHashSet<Character> set = new LinkedHashSet<Character>();
LinkedHashSet<Character> set = new LinkedHashSet<Character>();
for(int i=0;i<s.length();i++)
{
	set.add(s.charAt(i));
}

for(char c:set)
{

	int count=0;
	for(int i=0;i<s.length();i++)
{
	if(c==s.charAt(i))
	{
		count++;

	}
}
	System.out.print(c+" "+count);
}

	}

}
